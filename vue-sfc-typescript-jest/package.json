{
  "name": "vue-sfc-typescript-jest",
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.21",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.28",
    "autoprefixer": "^9.4.4",
    "browser-sync": "^2.26.3",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "eslint": "^5.11.1",
    "gulp": "^4.0.0",
    "gulp-cache": "^1.0.2",
    "gulp-dumber": "^0.7.0",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-tap": "^1.0.1",
    "gulp-typescript": "^5.0.0",
    "gulp-uglify-es": "^1.0.4",
    "gulp-vue-file": "^0.1.3",
    "jest": "^23.6.0",
    "jest-transform-stub": "^1.0.0",
    "node-sass": "^4.11.0",
    "postcss-url": "^8.0.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "vue-jest": "^3.0.2"
  },
  "scripts": {
    "lint": "tslint 'src/**/*.ts' 'test/**/*.ts'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ]
  }
}
